#include<iostream>
using namespace std;
class print{
	int n4;
	char *hg;
	public:
		print(int,char *);
		print(char *,int);
};
print::print(int a,char *b){
	n4=a;
	hg=b;
	cout<<n4;
	cout<<","<<hg<<endl;
}
print::print(char *c, int d){
	hg=c;
	n4=d;
	cout<<hg;
	cout<<","<<n4<<endl;
}
int main(){
	print d1(32,"adas"),d2("hehe",45);
	return 0;
}
